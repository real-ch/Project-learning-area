{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended","plugin: react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmmFeatures":{
      "jsx":true
    }
  },
  "plugins":["React"],
  "rules": {
    "semi":"error",
    "no-console": 0,
    "react/jsx-users-vars":"error"
  }
}
